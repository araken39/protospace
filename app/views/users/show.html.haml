.container.user-page
  %header.user-nav
    .media
      .media-left
        = link_to image_tag("#{current_user.avatar}",alt: "avatar",size:"64x64",class:"media-object"),"#"
      .media-body
        %h4#top-aligned-media.media-heading
          Top aligned media
          %span.label.label-default.project-number
            29
            %i Projects
        .proto-user
          %span.degree
            = current_user.member
        .row
          .col-md-6
            %h4 Profile
            = current_user.profile
          .col-md-6
            %h4 Works
            = current_user.works
  .row
    = render @user_prototypes

.text-center
  %ul.pagination
    %li.disabled
      %a{"aria-label" => "Previous", :href => "#"}
        %span{"aria-hidden" => "true"} «
    %li.active
      %a{:href => "#"}
        1
        %span.sr-only (current)
    %li
      %a{"aria-label" => "Next", :href => "#"}
        %span{"aria-hidden" => "true"} »
