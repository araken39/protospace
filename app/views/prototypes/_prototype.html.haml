.col-sm-4.col-md-3.proto-content
  .thumbnail
    = link_to image_tag(set_main_thumnail(prototype)),prototype_path(prototype)
    .caption
      %h3
        = prototype.title
      .proto-meta
        .proto-user
          =link_to "#{prototype.user.name}",user_path(prototype.user)
        .proto-posted
          = prototype.created_at
      %ul.proto-tag-list.list-inline
        -# タグはあとで実装するので今は無視
        %li
          %a{href: "#", class: "btn btn-default"} iPad
